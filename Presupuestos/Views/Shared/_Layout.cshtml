<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="description" content="Web Page for the Commercial Department" />
    <meta http-equiv="author" content="Laureano Gabriel Sánchez Aguilar" />
    <link rel="shortcut icon" type="image/x-icon" href="~/favicon.ico" />
    <link rel="icon" type="image/icon" href="~/favicon.ico" />
    <link rel="icon" type="image/png" href="~/favicon-32x32.png" sizes="32x32" />
    <link rel="icon" type="image/png" href="~/favicon-16x16.png" sizes="16x16" />
    <title>@ViewBag.Title - Pipeline Presupuestos</title>
    @Styles.Render("~/Content/css")
    <!--bootstrap.min.css-->
    <link rel="stylesheet" href="https://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
    <link rel="stylesheet" href="https://bootswatch.com/4/superhero/bootstrap.min.css"/>
    @Scripts.Render("~/bundles/modernizr")
    <!--modernizr-->
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.1/Chart.min.js"></script>
</head>
<body>
    <p id="Top"></p>
    <nav class="navbar fixed-top navbar-expand-lg navbar-dark bg-dark">
        @using (Html.BeginForm("Index", "Home", FormMethod.Get))
        {
            <button class="img-responsive" type="submit" style="width:232px; height:auto; background-color:#ffffff">
                <img src="~/Images/GrupoVargas.png" alt="Grupo Vargas S.A." title="Volver al menú principal" style="width:221px;height:auto" />
            </button>
        }
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarColor02" aria-controls="navbarColor02" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarColor02">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item dropdown active">
                    <a class="nav-link dropdown-toggle glyphicon glyphicon-dashboard" data-toggle="dropdown"
                       href="#" role="button" aria-haspopup="true" aria-expanded="false">Abastecimiento<span class="caret"></span></a>
                    <div class="dropdown-menu" x-placement="bottom-start" style="position: absolute; transform: translate3d(0px, 40px, 0px); top: 0px; left: 0px; will-change: transform;">
                        <a class="dropdown-item" href="~/Commercial/Pipeline">Pipeline</a>
                        <a class="dropdown-item" href="~/Session/Imports">Importar</a>
                    </div>
                </li>
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle glyphicon glyphicon-th-list" data-toggle="dropdown"
                       href="#" role="button" aria-haspopup="true" aria-expanded="false">Ventas<span class="caret"></span></a>
                    <div class="dropdown-menu" x-placement="bottom-start" style="position: absolute; transform: translate3d(0px, 40px, 0px); top: 0px; left: 0px; will-change: transform;">
                        <a class="dropdown-item" href="~/Pipeline/detalle">Detalle Pipeline</a>
                        <a class="dropdown-item" href="~/Pipeline/analisisMeses">Análisis Meses</a>
                        <a class="dropdown-item" href="~/Pipeline/resumenPorEjecutivo">Resumen por Ejecutivo</a>
                    </div>
                </li>
   
            </ul>
            <ul class="nav navbar-nav navbar-right">
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle glyphicon glyphicon-info-sign" data-toggle="dropdown"
                    href="#" role="button" aria-haspopup="true" aria-expanded="false">Información<span class="caret"></span></a>
                    <div class="dropdown-menu" x-placement="bottom-start" style="position: absolute; transform: translate3d(0px, 40px, 0px); top: 0px; left: 0px; will-change: transform;">
                        <a class="dropdown-item" href="~/Home/About">Acerca</a>
                        <a class="dropdown-item" href="~/Home/Tutorial">Tutorial</a>
                    </div>
                </li>
                <li class="nav-item">
                <a class="nav-link" href="http://intranet/gv/DefaultPage.aspx">Regresar</a>
                </li>
            </ul>
        </div>
    </nav>

    <div class="container body-content" style="background-color:#ffffff">
        @RenderBody()
        <hr />
        <footer class="footer">
                <p>@DateTime.Now.Year - Grupo Vargas S.A.</p>
                <p><em>Versión 2.6</em></p>
        </footer>
    </div>
    <!--Scripts.Render("~/bundles/jquery")-->
    <!--jquery.js-->
    <script src="https://code.jquery.com/jquery-3.3.1.slim.js"
            integrity="sha256-fNXJFIlca05BIO2Y5zh1xrShK3ME+/lYZ0j+ChxX2DA="
            crossorigin="anonymous"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
    <!--Scripts.Render("~/bundles/bootstrap")-->
    <!--bootstrap.min.js-->
    <script type="text/javascript" src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"></script>
    <!--respond.js-->
    <script type="text/javascript" src="http://airve.github.io/js/response/response.js"></script>
    <script src="~/Scripts/datepicker-es.js"></script>
    <script src="~/Scripts/presupuestos.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
